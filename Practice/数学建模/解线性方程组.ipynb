   "source": [
    "#  求解线性方程组\n",
    "\n",
    "from scipy import linalg\n",
    "import numpy as np\n",
    "\n",
    "# x1 + x2 + 7*x3 = 2\n",
    "# 2*x1 + 3*x2 + 5*x3 = 3\n",
    "# 4*x1 + 2*x2 + 6*x3 = 4\n",
    "\n",
    "A = np.array([[1, 1, 7], [2, 3, 5], [4, 2, 6]])  # A代表系数矩阵\n",
    "b = np.array([2, 3, 4])  # b代表常数列\n",
    "x = linalg.solve(A, b)\n",
    "print(x)"
   ]
  }
